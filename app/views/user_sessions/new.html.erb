<h1>ログイン</h1>
<%= form_with(url: login_path) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit 'ログイン', class: "btn btn-primary" %>
  </div>
<% end %>
  <%= link_to '登録ページへ', new_user_path%>
